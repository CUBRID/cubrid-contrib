<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC "-//ibatis.apache.org//DTD SQL Map 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-2.dtd">

<sqlMap namespace="srch.stat">

	<!--
		폐기현황 일반통계
		: 기록물철 (1.폐기년도-보존기간)
	-->
	<resultMap id="findDisuseCurStateNomalStatYearTermFolderList-result" class="rms.srch.stat.domain.DisuseCurState">
	    <result property="title"		column="disuse_ymd"			/>
	    <result property="total"		column="total" 				/>
	    <result property="sumA"			column="sum_a"   			/>
	    <result property="sumB"			column="sum_b"  			/>
	    <result property="sumC"			column="sum_c"  			/>
	    <result property="sumD"			column="sum_d"   			/>
	    <result property="sumE"			column="sum_e"   			/>
	    <result property="sumF"			column="sum_f"  			/>
	    <result property="sumG"			column="sum_g"   			/>
	</resultMap>
	<select id="findDisuseCurStateNomalStatYearTermFolderTotal" parameterClass="rms.srch.stat.domain.DisuseCurState" resultMap="findDisuseCurStateNomalStatYearTermFolderList-result">
		SELECT '총계' AS disuse_ymd
			, NVL(SUM((CASE WHEN A.presv_term_cd IN ('01','03','05','10','20','25','30','40') THEN 1 ELSE 0 END)),0) AS total
			, NVL(SUM((CASE WHEN A.presv_term_cd='01' THEN 1 ELSE 0 END)),0) AS sum_a
			, NVL(SUM((CASE WHEN A.presv_term_cd='03' THEN 1 ELSE 0 END)),0) AS sum_b
			, NVL(SUM((CASE WHEN A.presv_term_cd='05' THEN 1 ELSE 0 END)),0) AS sum_c
			, NVL(SUM((CASE WHEN A.presv_term_cd='10' THEN 1 ELSE 0 END)),0) AS sum_d
			, NVL(SUM((CASE WHEN A.presv_term_cd IN ('20','25') THEN 1 ELSE 0 END)),0) AS sum_e
			, NVL(SUM((CASE WHEN A.presv_term_cd='30' THEN 1 ELSE 0 END)),0) AS sum_f
			, NVL(SUM((CASE WHEN A.presv_term_cd='40' THEN 1 ELSE 0 END)),0) AS sum_g
		FROM  TB_NKFOLDER A
		WHERE A.record_center_id = #recordCenterId:VARCHAR#
		<dynamic>
			<isNotEmpty prepend="AND" property="orgCd">
				A.org_cd = #orgCd:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="disuseFromYyyy">
				<isNotEmpty property="disuseToYyyy">
					(SUBSTR(A.disuse_ymd,1,4) BETWEEN #disuseFromYyyy:VARCHAR# AND #disuseToYyyy:VARCHAR#)
				</isNotEmpty>
				<isEmpty property="disuseToYyyy">
					SUBSTR(A.disuse_ymd,1,4) <![CDATA[>=]]> #disuseFromYyyy:VARCHAR#
				</isEmpty>
			</isNotEmpty>
			<isEmpty prepend="AND" property="disuseFromYyyy">
				<isNotEmpty property="disuseToYyyy">
					SUBSTR(A.disuse_ymd,1,4) <![CDATA[<=]]> #disuseToYyyy:VARCHAR#
				</isNotEmpty>
			</isEmpty>
			<isNotEmpty prepend="AND" property="creatSysCd">
				A.creat_sys_cd = #creatSysCd:VARCHAR#
			</isNotEmpty>
		</dynamic>
		AND   A.folder_keep_state_cd = '03'
	</select>
	<select id="findDisuseCurStateNomalStatYearTermFolderList" parameterClass="rms.srch.stat.domain.DisuseCurState" resultMap="findDisuseCurStateNomalStatYearTermFolderList-result">
		SELECT SUBSTR(A.disuse_ymd,1,4) AS disuse_ymd
			, NVL(SUM((CASE WHEN A.presv_term_cd IN ('01','03','05','10','20','25','30','40') THEN 1 ELSE 0 END)),0) AS total
			, NVL(SUM((CASE WHEN A.presv_term_cd='01' THEN 1 ELSE 0 END)),0) AS sum_a
			, NVL(SUM((CASE WHEN A.presv_term_cd='03' THEN 1 ELSE 0 END)),0) AS sum_b
			, NVL(SUM((CASE WHEN A.presv_term_cd='05' THEN 1 ELSE 0 END)),0) AS sum_c
			, NVL(SUM((CASE WHEN A.presv_term_cd='10' THEN 1 ELSE 0 END)),0) AS sum_d
			, NVL(SUM((CASE WHEN A.presv_term_cd IN ('20','25') THEN 1 ELSE 0 END)),0) AS sum_e
			, NVL(SUM((CASE WHEN A.presv_term_cd='30' THEN 1 ELSE 0 END)),0) AS sum_f
			, NVL(SUM((CASE WHEN A.presv_term_cd='40' THEN 1 ELSE 0 END)),0) AS sum_g
		FROM  TB_NKFOLDER A
		WHERE A.record_center_id = #recordCenterId:VARCHAR#
		<dynamic>
			<isNotEmpty prepend="AND" property="orgCd">
				A.org_cd = #orgCd:VARCHAR#
			</isNotEmpty>
			<isNotEmpty prepend="AND" property="disuseFromYyyy">
				<isNotEmpty property="disuseToYyyy">
					(SUBSTR(A.disuse_ymd,1,4) BETWEEN #disuseFromYyyy:VARCHAR# AND #disuseToYyyy:VARCHAR#)
				</isNotEmpty>
				<isEmpty property="disuseToYyyy">
					SUBSTR(A.disuse_ymd,1,4) <![CDATA[>=]]> #disuseFromYyyy:VARCHAR#
				</isEmpty>
			</isNotEmpty>
			<isEmpty prepend="AND" property="disuseFromYyyy">
				<isNotEmpty property="disuseToYyyy">
					SUBSTR(A.disuse_ymd,1,4) <![CDATA[<=]]> #disuseToYyyy:VARCHAR#
				</isNotEmpty>
			</isEmpty>
			<isNotEmpty prepend="AND" property="creatSysCd">
				A.creat_sys_cd = #creatSysCd:VARCHAR#
			</isNotEmpty>
		</dynamic>
		AND   A.folder_keep_state_cd = '03'
		GROUP BY SUBSTR(A.disuse_ymd,1,4)
		ORDER BY disuse_ymd
	</select>

