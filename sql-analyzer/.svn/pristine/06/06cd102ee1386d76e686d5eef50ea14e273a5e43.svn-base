	<select id="findDisuseCurStateNomalStatTermTypeFolderTotal" parameterClass="rms.srch.stat.domain.DisuseCurState" resultMap="findDisuseCurStateNomalStatTermTypeFolderList-result">
		SELECT '총계' AS presv_term_cd_nm
			, NVL(SUM((CASE WHEN ((A.folder_detl_type_cd='01' AND A.folder_type_div_cd='1') OR (A.folder_detl_type_cd='02' AND A.folder_type_div_cd IN ('1','2','3','4','5'))) THEN 1 ELSE 0 END)),0) AS total
