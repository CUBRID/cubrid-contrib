	  	DELETE 	  	FROM TB_STMEDIUM	  	WHERE RECORD_CENTER_ID = '1'  	  		AND MEDIUM_ID = '1';


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\xml\MediumDao.xml
-- Query    : update
-- Query Id : setUseSizeMedium

	  	UPDATE TB_STMEDIUM	  	SET USE_SIZE = '1' + 	  		(SELECT USE_SIZE FROM TB_STMEDIUM 	  		WHERE RECORD_CENTER_ID = '1'	  			AND MEDIUM_ID  = '1' 	  			AND DEL_FLAG = '1')	  	WHERE RECORD_CENTER_ID = '1'  	  		AND MEDIUM_ID  = '1'	  		AND DEL_FLAG = '1'	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\xml\MediumDao.xml
-- Query    : update
-- Query Id : updateMedium

	  	UPDATE TB_STMEDIUM	  	SET  VOLM_ID    = '1'       			,MEDIUM_NM  = '1'     			,MEDIUM_CD  = '1'     			,MEDIUM_DETAIL_CD = '1'     			,MEDIUM_PATH= '1'   			,TOT_SIZE   = '1'      			,CHNG_DTIME = TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS')    			,REMK       = '1'        	  	WHERE RECORD_CENTER_ID = '1'  	  		AND MEDIUM_ID = '1'	;


