		UPDATE TB_STVOLM		SET VOLM_NM  = '1'	   		,CHNG_DTIME = TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS')	   		,REMK = '1'		WHERE RECORD_CENTER_ID = '1'  			AND VOLM_ID = '1';


