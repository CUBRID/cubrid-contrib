	  	UPDATE TB_STMEDIUM	  	SET DEL_FLAG = '1'	  	   	,CHNG_DTIME = TO_CHAR(SYSDATE,'YYYYMMDDHH24MISS')	  	WHERE RECORD_CENTER_ID = '1'  	  		AND MEDIUM_ID = '1'	;


