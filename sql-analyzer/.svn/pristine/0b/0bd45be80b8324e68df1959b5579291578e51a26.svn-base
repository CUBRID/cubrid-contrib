-- Query    : update
-- Query Id : setDelflagVolume

		UPDATE TB_STVOLM		SET DEL_FLAG = '1'			,CHNG_DTIME = TO_CHAR(CAST(SYSDATE AS DATETIME),'YYYYMMDDHH24MISS')					WHERE RECORD_CENTER_ID = '1'			AND VOLM_ID = '1';


