-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon_source\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestPdf.xml
-- Query    : update
-- Query Id : UpdatePdf_RdFolder

		UPDATE TB_RDFOLDER C		SET C.PDF_A1_TRANS_STATE = (                   SELECT (CASE WHEN B.CNT4 + B.CNT6 = B.F_CNT THEN '4' 			                     WHEN B.CNT1 + B.CNT2 + B.CNT3 > 0 THEN '3' 			                     WHEN B.CNT5 > 0 THEN '5' 			                     ELSE '0'			                END) AS PDF_A1_TRANS_STATE 			        FROM (				        SELECT COUNT(1) AS F_CNT				            , NVL(SUM(A.CNT0),0) AS CNT0  				            , NVL(SUM(A.CNT1),0) AS CNT1  				            , NVL(SUM(A.CNT2),0) AS CNT2  				            , NVL(SUM(A.CNT3),0) AS CNT3  				            , NVL(SUM(A.CNT4),0) AS CNT4  				            , NVL(SUM(A.CNT5),0) AS CNT5  				            , NVL(SUM(A.CNT6),0) AS CNT6 				        FROM ( 				            SELECT					              DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'0',1,0) CNT0 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'1',1,0) CNT1 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'2',1,0) CNT2 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'3',1,0) CNT3 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'4',1,0) CNT4 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'5',1,0) CNT5 					            , DECODE(NVL(PDF_A1_TRANS_STATE,'0'),'6',1,0) CNT6 				            FROM  TB_RDRECORD A 				            WHERE RECORD_CENTER_ID = '1'				            AND FOLDER_ID = '1'				            AND   ERECORD_FLAG = '1' 				            AND   ATTACH_CNT > 0 				        ) A			        ) B			    )		WHERE RECORD_CENTER_ID = '1'		AND FOLDER_ID = '1'	;


