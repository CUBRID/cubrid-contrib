-- Query    : insert
-- Query Id : InsertPdf_StReqFile

		INSERT INTO TB_STREQFILE		(		    RECORD_CENTER_ID, JOB_ID, SRC_FILE_ID, REQ_ORD, SRC_FILE_NM, SRC_STORE_FILE_ID, SRC_FILE_SIZE, SRC_STORAGE_TYPE, SRC_STORAGE_VENDOR		)		(		    SELECT D.RECORD_CENTER_ID, D.JOB_ID, D.ORIGN_FILE_ID		        , ROWNUM AS REQ_ORD, D.FILE_NM		        , (CASE WHEN D.SRC_STORAGE_TYPE='01' THEN D.ORIGN_FILE_ID ELSE D.STORE_FILE_ID END) AS SRC_STORE_FILE_ID		        , D.FILE_SIZE, D.SRC_STORAGE_TYPE, D.SRC_STORAGE_VENDOR		    FROM (		        SELECT C.RECORD_CENTER_ID, '1' AS JOB_ID, C.ORIGN_FILE_ID			        , 0 AS REQ_ORD, C.FILE_NM, C.STORE_FILE_ID, C.FILE_SIZE			        , (SELECT E.MEDIUM_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_TYPE			        , (SELECT E.MEDIUM_DETAIL_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_VENDOR		        FROM  TB_STORIGNFILE C		        WHERE (C.RECORD_CENTER_ID||C.ORIGN_ID)		        IN (		            SELECT A.RECORD_CENTER_ID||(SELECT B.ORIGN_ID FROM TB_RDRECORD B WHERE B.RECORD_ID=A.RECORD_ID) AS ORIGN_ID		            FROM  TB_STREQTRANS A		            WHERE A.RECORD_CENTER_ID = '1'		              AND A.JOB_ID = '1'		        )		        AND   C.DOC_TYPE_CD NOT IN ('10','S1')		        AND   C.PDF_A1_TRANS_STATE != '4'		        ORDER BY C.RECORD_CENTER_ID, C.ORIGN_ID, C.DOC_TYPE_CD, C.DOC_TYPE_SNO		    ) D		)	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon_source\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestPdf.xml
-- Query    : select
-- Query Id : SelectPdf_StReqFile_Ext

		SELECT C.RECORD_CENTER_ID, C.JOB_ID, C.SRC_FILE_ID, C.FILE_EXT, C.EXT_FLAG		FROM (		    SELECT A.RECORD_CENTER_ID, A.JOB_ID, A.SRC_FILE_ID, A.EXT AS FILE_EXT, 			(CASE WHEN A.EXT IS NOT NULL AND B.FILE_EXT IS NULL THEN 'X' ELSE 'O' END) AS EXT_FLAG		    FROM		    (		        SELECT RECORD_CENTER_ID, JOB_ID, SRC_FILE_ID, SRC_FILE_NM		        	, UPPER(CASE WHEN INSTR(SRC_FILE_NM,'.') > 0 THEN SUBSTR(SRC_FILE_NM, INSTR(SRC_FILE_NM,'.',,-(LENGTH(REPLACE(SRC_FILE_NM,'.','..')) - LENGTH(SRC_FILE_NM)))+1) ELSE '' END) AS EXT		        FROM TB_STREQFILE				WHERE JOB_ID = '1'				  AND RECORD_CENTER_ID = '1'		    ) A		    , (		        SELECT * FROM TB_STFILEEXTMNG		         WHERE FILE_TRANS_FLAG = '1'				   AND RECORD_CENTER_ID = '1'		    ) B		    WHERE A.EXT = B.FILE_EXT(+)		) C	;


