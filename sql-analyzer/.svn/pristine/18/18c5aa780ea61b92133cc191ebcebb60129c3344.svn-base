		SELECT E.record_center_id			, E.record_id			, E.creat_reg_dtime			, E.record_detl_type_cd			, E.record_detl_type_nm			, E.duty_chrgr_nm			, E.org_cd			, E.org_nm			, E.org_abbr_nm			, E.title			, E.attach_cnt			, E.oper_read_range_cd		FROM (			SELECT D.*			FROM (				SELECT ROWNUM AS rnum					, C.*				FROM (					SELECT B.*					FROM (						SELECT /*+ index(A, TB_NKRECORD_IX01) */						       A.record_center_id						     , A.record_id						     , RPAD(A.creat_reg_dtime,14,'0') creat_reg_dtime						     , A.creat_reg_no							 , A.record_detl_type_cd							 , DECODE(A.record_detl_type_cd,'01','문서','02','메모','03','회의','') record_detl_type_nm							 , '' duty_chrgr_nm 							 , A.org_cd			                 , (SELECT B.org_nm FROM TB_STORG B WHERE B.record_center_id=A.record_center_id AND B.org_cd=A.org_cd) org_nm			                 , (SELECT B.org_abbr_nm FROM TB_STORG B WHERE B.record_center_id=A.record_center_id AND B.org_cd=A.org_cd) org_abbr_nm							 , A.title							 , TO_NUMBER(NVL(A.attach_cnt,'0')) attach_cnt							 , A.oper_read_range_cd						FROM  TB_NKRECORD A						WHERE A.record_center_id = '1'						AND   A.folder_id = '1'						 AND 							A.prod_type_cd = '1'						 					) B					 WHERE  						B.title LIKE '1'						  AND 						B.title LIKE '1'						  AND 						B.creat_reg_dtime >= '1'||'000000'						  AND 						B.creat_reg_dtime <= '1'||'240000'						  AND 						B.duty_chrgr_nm LIKE '1'						  AND 						B.org_abbr_nm LIKE '1'						  					ORDER BY B.record_center_id, B.creat_reg_dtime, B.creat_reg_no				) C				WHERE ROWNUM <= '1'			) D			WHERE D.rnum >= '1'		) E		ORDER BY E.rnum	;


