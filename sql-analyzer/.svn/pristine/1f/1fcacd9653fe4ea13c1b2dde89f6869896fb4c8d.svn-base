<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings 
		cacheModelsEnabled="true" 
		enhancementEnabled="true"
		lazyLoadingEnabled="true" 
		maxRequests="128" 
		maxSessions="10"
		maxTransactions="5" 
		useStatementNamespaces="true"
		defaultStatementTimeout="1000"
	/>

	<!-- 8.sys -->
	<!-- sys.sys -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/sys/User.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/sys/MenuMng.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/sys/MenuGrantGroup.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/sys/DownloadIp.xml" />
	<!-- sys.confset -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/confset/CodeApply.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/confset/ConfSet.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/confset/FormatServerMng.xml" />
	<!-- sys.rmsstate -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/rmsstate/RmsState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/rmsstate/RmsEquip.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/rmsstate/RmsStateReport.xml" />
	<!-- sys.mig -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/mig/Mig.xml" />
	<!-- sys.recordmng -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/recordmng/OpenMng.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/recordmng/ReadRangeMng.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/recordmng/CurKeepOrgMng.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/sys/recordmng/OrgChngHistMng.xml" />
</sqlMapConfig>