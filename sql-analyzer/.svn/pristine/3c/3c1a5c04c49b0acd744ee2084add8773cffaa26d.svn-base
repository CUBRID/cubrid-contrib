<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings 
		cacheModelsEnabled="true" 
		enhancementEnabled="true"
		lazyLoadingEnabled="true" 
		maxRequests="128" 
		maxSessions="10"
		maxTransactions="5" 
		useStatementNamespaces="true"
		defaultStatementTimeout="1000"
	/>

	<!-- 10.rmsstate -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/rmsstate/curstate/CurStateNoml.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/rmsstate/curstate/CurStateEquip.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/rmsstate/curstate/CurStateKeep.xml" />
</sqlMapConfig>