<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings 
		cacheModelsEnabled="true" 
		enhancementEnabled="true"
		lazyLoadingEnabled="true" 
		maxRequests="128" 
		maxSessions="10"
		maxTransactions="5" 
		useStatementNamespaces="true"
		defaultStatementTimeout="1000"
	/>

	<!-- 2.presv -->
	<!-- 포멧변환 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/format/PresvFormatPdf.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/format/PresvFormatNeo.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/format/PresvFormatNeoReconv.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/format/PresvFormatMonitor.xml" />
	
	<!-- 기록정리 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/adjst/Adjst.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/adjst/VirusCheck.xml" />
	
	<!-- 기록물건정리(비전자) -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/clssdiv/ClssDiv.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/clssdiv/RecordTitleMod.xml" />
	
	<!-- 서고관리 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/booksf/Booksf.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/booksf/BooksfCntChk.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/booksf/CarryInOut.xml" />
	
	<!-- 디지털변환 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/scan/Scan.xml" />
	
	<!-- 매체수록 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/optidiskmst/OptidiskMst.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/optidiskmst/OptidiskCntChk.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/media/PresvMf.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/optidiskmst/OptidiskMng.xml" />
	
	<!-- RFID관리 -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/rfid/RfidMng.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/presv/rfid/RfidTagPublication.xml" />
	
</sqlMapConfig>