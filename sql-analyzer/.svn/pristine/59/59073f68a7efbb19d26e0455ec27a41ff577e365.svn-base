		SELECT A.take_rep_cd, A.take_yyyy, A.take_sno, A.record_center_id, A.record_id			, A.folder_id, A.rd_record_id, A.rd_folder_id		FROM (	        SELECT C.*	        FROM  TB_RCTAKERECORDLIST C	        WHERE C.take_rep_cd = '1'	        AND   C.take_yyyy = '1'	        AND   C.take_sno = '1'	        AND   C.record_center_id = '1'	        AND   C.folder_id = '1'	        ORDER BY record_center_id, record_id	    ) A;


