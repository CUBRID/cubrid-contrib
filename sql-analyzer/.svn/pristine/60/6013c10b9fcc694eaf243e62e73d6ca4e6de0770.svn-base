<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings 
		cacheModelsEnabled="true" 
		enhancementEnabled="true"
		lazyLoadingEnabled="true" 
		maxRequests="128" 
		maxSessions="10"
		maxTransactions="5" 
		useStatementNamespaces="true"
		defaultStatementTimeout="1000"
	/>

	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/accsset/AccsSetFolder.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/accsset/AccsSetRecord.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/accsset/AccsSetOrg.xml" />
	
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/DocTrace.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/Folder.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/FolderTrace.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/Record.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/RecordTrace.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/Trace.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/TraceDataDoc.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/UserTrace.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/trace/WorkLevlTrace.xml" />

	<sqlMap resource="egovframework/sqlmap/rms/default/rms/accs/verify/VerifyStatus.xml" />

</sqlMapConfig>