-- Query    : insert
-- Query Id : InsertRecordNeo_StReqFile_BefRecordNeoFile

		INSERT INTO TB_STREQFILE		(		    RECORD_CENTER_ID, JOB_ID, SRC_FILE_ID, REQ_ORD, SRC_FILE_NM, SRC_STORE_FILE_ID, SRC_FILE_SIZE, SRC_STORAGE_TYPE, SRC_STORAGE_VENDOR		)		(		    SELECT D.RECORD_CENTER_ID, D.JOB_ID, D.ORIGN_NEO_FILE_ID		        , ROWNUM AS REQ_ORD, D.FILE_NM		        , (CASE WHEN D.SRC_STORAGE_TYPE='01' THEN D.ORIGN_NEO_FILE_ID ELSE D.STORE_FILE_ID END) AS SRC_STORE_FILE_ID		        , D.FILE_SIZE, D.SRC_STORAGE_TYPE, D.SRC_STORAGE_VENDOR		    FROM (		        SELECT C.RECORD_CENTER_ID, '1' AS JOB_ID, C.ORIGN_NEO_FILE_ID			        , 0 AS REQ_ORD, C.FILE_NM, C.STORE_FILE_ID, C.FILE_SIZE			        , (SELECT E.MEDIUM_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_TYPE			        , (SELECT E.MEDIUM_DETAIL_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_VENDOR		        FROM  TB_STORIGNNEOFILE C		        WHERE 		        EXISTS (		            SELECT A.RECORD_CENTER_ID, B.ORIGN_ID                    FROM   TB_STREQTRANS A, TB_RDRECORD B                    WHERE  A.RECORD_CENTER_ID = '1'                    AND    A.JOB_ID = '1'                    AND    B.RECORD_ID = A.RECORD_ID                    AND    A.RECORD_CENTER_ID = C.RECORD_CENTER_ID                    AND    B.ORIGN_ID = C.ORIGN_ID		        )		        AND C.CHNG_SNO = (SELECT MAX(CHNG_SNO)                                  FROM   TB_STORIGNNEOFILE Z                                  WHERE                                     EXISTS (                                         SELECT A.RECORD_CENTER_ID, B.ORIGN_ID                                         FROM   TB_STREQTRANS A, TB_RDRECORD B                                         WHERE  A.RECORD_CENTER_ID = '1'                                         AND    A.JOB_ID = '1'                                         AND    B.RECORD_ID = A.RECORD_ID                                         AND    A.RECORD_CENTER_ID = Z.RECORD_CENTER_ID                                         AND    B.ORIGN_ID = Z.ORIGN_ID                                  ))		    ) D		);


