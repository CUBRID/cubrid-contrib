-- Query    : update
-- Query Id : UpdateRecordNeo_StReqTrans_Fail

		UPDATE TB_STREQTRANS		SET   REQUEST_STATUS = 'D'			, ERROR_CODE = 'R1001'			, ERROR_CONTENTS = '1'		WHERE JOB_ID = '1'		AND   REQUEST_STATUS = '-';


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestRecordNeo.xml
-- Query    : insert
-- Query Id : InsertRecordNeo_StReqFile_OrignFile

		INSERT INTO TB_STREQFILE		(		    RECORD_CENTER_ID, JOB_ID, SRC_FILE_ID, REQ_ORD, SRC_FILE_NM, SRC_STORE_FILE_ID, SRC_FILE_SIZE, SRC_STORAGE_TYPE, SRC_STORAGE_VENDOR		)		(		    SELECT D.RECORD_CENTER_ID, D.JOB_ID, D.ORIGN_FILE_ID		        , ROWNUM AS REQ_ORD, D.FILE_NM		        , (CASE WHEN D.SRC_STORAGE_TYPE='01' THEN D.ORIGN_FILE_ID ELSE D.STORE_FILE_ID END) AS SRC_STORE_FILE_ID		        , D.FILE_SIZE, D.SRC_STORAGE_TYPE, D.SRC_STORAGE_VENDOR		    FROM (		        SELECT C.RECORD_CENTER_ID, '1' AS JOB_ID, C.ORIGN_FILE_ID			        , 0 AS REQ_ORD, C.FILE_NM, C.STORE_FILE_ID, C.FILE_SIZE			        , (SELECT E.MEDIUM_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_TYPE			        , (SELECT E.MEDIUM_DETAIL_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_VENDOR		        FROM  TB_STORIGNFILE C		        WHERE 		        EXISTS (		            SELECT A.RECORD_CENTER_ID		                , (SELECT B.ORIGN_ID FROM tb_strecordorign B WHERE b.record_center_id = a.record_center_id and B.RECORD_ID=A.RECORD_ID and b.erecord_flag = '1') AS ORIGN_ID		            FROM  TB_STREQTRANS A		            WHERE A.RECORD_CENTER_ID = '1'		            AND A.JOB_ID = '1'                            AND A.RECORD_CENTER_ID = C.RECORD_CENTER_ID                            AND ORIGN_ID = C.ORIGN_ID		        )		        AND   C.DOC_TYPE_CD NOT IN ('10','S1')		        ORDER BY C.RECORD_CENTER_ID, C.ORIGN_ID, C.DOC_TYPE_CD, C.DOC_TYPE_SNO		    ) D		)	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestRecordNeo.xml
-- Query    : insert
-- Query Id : InsertRecordNeo_StReqFile_PdfFile

		INSERT INTO TB_STREQFILE		(		    RECORD_CENTER_ID, JOB_ID, SRC_FILE_ID, REQ_ORD, SRC_FILE_NM, SRC_STORE_FILE_ID, SRC_FILE_SIZE, SRC_STORAGE_TYPE, SRC_STORAGE_VENDOR		)		(		    SELECT D.RECORD_CENTER_ID, D.JOB_ID, D.PDF_A1_FILE_ID		        , ROWNUM AS REQ_ORD, D.FILE_NM		        , (CASE WHEN D.SRC_STORAGE_TYPE='01' THEN D.PDF_A1_FILE_ID ELSE D.STORE_FILE_ID END) AS SRC_STORE_FILE_ID		        , D.FILE_SIZE, D.SRC_STORAGE_TYPE, D.SRC_STORAGE_VENDOR		    FROM (		        SELECT C.RECORD_CENTER_ID, '1' AS JOB_ID, C.PDF_A1_FILE_ID			        , 0 AS REQ_ORD, C.FILE_NM, C.STORE_FILE_ID, C.FILE_SIZE			        , (SELECT E.MEDIUM_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_TYPE			        , (SELECT E.MEDIUM_DETAIL_CD FROM TB_STMEDIUM E WHERE E.RECORD_CENTER_ID=C.RECORD_CENTER_ID AND E.MEDIUM_ID=C.MEDIUM_ID) AS SRC_STORAGE_VENDOR		        FROM  TB_STORIGNPDFA1FILE C		        WHERE 		        EXISTS (		            SELECT A.RECORD_CENTER_ID		                , (SELECT B.ORIGN_ID FROM tb_strecordorign B WHERE b.record_center_id = a.record_center_id and B.RECORD_ID=A.RECORD_ID and b.erecord_flag = '1') AS ORIGN_ID		            FROM  TB_STREQTRANS A		            WHERE A.RECORD_CENTER_ID = '1'		            AND A.JOB_ID = '1'		            AND A.RECORD_CENTER_ID = C.RECORD_CENTER_ID		            AND ORIGN_ID = C.ORIGN_ID		        )		        ORDER BY C.RECORD_CENTER_ID, C.ORIGN_ID, C.DOC_TYPE_CD, C.DOC_TYPE_SNO		    ) D		);


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestRecordNeo.xml
-- Query    : select
-- Query Id : SelectRecordNeo_RecordPdfNotCnt

		SELECT COUNT(1) AS CNT		FROM  TB_STORIGNFILE C		WHERE EXISTS (		    SELECT 'X'		    FROM  TB_STREQTRANS A, TB_RDRECORD B		    WHERE A.RECORD_CENTER_ID = '1'		      AND A.JOB_ID = '1'		    		    AND A.ERECORD_FLAG = '1'		    AND A.RECORD_CENTER_ID = C.RECORD_CENTER_ID		    AND B.ORIGN_ID = C.ORIGN_ID			AND B.RECORD_ID=A.RECORD_ID		)		AND   C.PDF_A1_TRANS_STATE NOT IN ('4','6');


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid_daemon\rmsPresvFormat\conf\sqlMap\sql\sqlMap-RequestRecordNeo.xml
-- Query    : select
-- Query Id : SelectRecordNeo_BefRecordNeoFile

		SELECT C.FILE_NM		FROM  TB_STORIGNNEOFILE C		WHERE 		EXISTS (		    SELECT A.RECORD_CENTER_ID, B.ORIGN_ID            FROM   TB_STREQTRANS A, TB_RDRECORD B            WHERE  A.RECORD_CENTER_ID = '1'            AND    A.JOB_ID = '1'            AND    B.RECORD_ID = A.RECORD_ID            AND    A.RECORD_CENTER_ID = C.RECORD_CENTER_ID            AND    B.ORIGN_ID = C.ORIGN_ID		)		AND C.CHNG_SNO = (SELECT MAX(CHNG_SNO)                          FROM   TB_STORIGNNEOFILE Z                          WHERE                            EXISTS (                                 SELECT A.RECORD_CENTER_ID, B.ORIGN_ID                                 FROM   TB_STREQTRANS A, TB_RDRECORD B                                 WHERE  A.RECORD_CENTER_ID = '1'                                 AND    A.JOB_ID = '1'                                 AND    B.RECORD_ID = A.RECORD_ID                                 AND    A.RECORD_CENTER_ID = Z.RECORD_CENTER_ID                                 AND    B.ORIGN_ID = Z.ORIGN_ID                          )                          AND NVL(DEL_FLAG, '0') = '0')	;


