     , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-1),4,'0') THEN 1 ELSE 0 END)),0) AS sum_i		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-0),4,'0') THEN 1 ELSE 0 END)),0) AS sum_j		            FROM  TB_RDFOLDER A		            WHERE A.record_center_id = '1'		            AND   A.creat_sys_cd = '1'		            AND  (A.creat_yyyy BETWEEN LPAD(TO_CHAR(TO_NUMBER('1')-9),4,'0') AND '1') AND 						A.org_cd = '1'					  AND 			        	A.folder_type_div_cd = '1' AND A.folder_detl_type_cd = '01'			         					AND   A.folder_keep_state_cd IN ('01','04')		            GROUP BY A.folder_type_div_cd, A.folder_detl_type_cd		        ) C		         WHERE  			        	B.com_cd = C.folder_detl_type_cd(+)			          AND 			        	B.com_cd = C.folder_detl_type_cd(+)			          		    )		    UNION ALL		    (		        SELECT '2' AS div_cd		            , B.com_cd		            , B.com_cd_nm		            , (C.sum_a+C.sum_b+C.sum_c+C.sum_d+C.sum_e+C.sum_f+C.sum_g+C.sum_h+C.sum_i+C.sum_j) AS total		            , C.sum_a		            , C.sum_b		            , C.sum_c		            , C.sum_d		            , C.sum_e		            , C.sum_f		            , C.sum_g		            , C.sum_h		            , C.sum_i		            , C.sum_j		        FROM 			        (SELECT com_cd, com_cd_nm FROM TB_ZZCOMCD WHERE com_type_cd='RD03' AND com_cd IN ('01','02','03','04')) B			         		        , (		            SELECT A.record_detl_type_cd		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-9),4,'0') THEN 1 ELSE 0 END)),0) AS sum_a		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-8),4,'0') THEN 1 ELSE 0 END)),0) AS sum_b		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-7),4,'0') THEN 1 ELSE 0 END)),0) AS sum_c		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-6),4,'0') THEN 1 ELSE 0 END)),0) AS sum_d		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-5),4,'0') THEN 1 ELSE 0 END)),0) AS sum_e		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-4),4,'0') THEN 1 ELSE 0 END)),0) AS sum_f		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-3),4,'0') THEN 1 ELSE 0 END)),0) AS sum_g		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-2),4,'0') THEN 1 ELSE 0 END)),0) AS sum_h		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-1),4,'0') THEN 1 ELSE 0 END)),0) AS sum_i		                , NVL(SUM((CASE WHEN A.creat_yyyy=LPAD(TO_CHAR(TO_NUMBER('1')-0),4,'0') THEN 1 ELSE 0 END)),0) AS sum_j		            FROM  TB_RDRECORD A		            	, TB_RDFOLDER F		            WHERE A.record_center_id = '1'		            AND   A.creat_sys_cd = '1'		            AND  (A.creat_yyyy BETWEEN LPAD(TO_CHAR(TO_NUMBER('1')-9),4,'0') AND '1') AND 						A.org_cd = '1'					  AND 			        	A.record_detl_type_cd IN ('01','02','03','04')			         			        AND   F.record_center_id = A.record_center_id			        AND   F.folder_id = A.folder_id					AND   F.folder_keep_state_cd IN ('01','04')		            GROUP BY A.record_detl_type_cd 		        ) C		        WHERE B.com_cd = C.record_detl_type_cd(+)		    )		) D		ORDER BY D.div_cd, D.com_cd	;


