<?xml version="1.0" encoding="euc-kr"?>
<configuration>
	<!-- RMS Configuration -->
	<rms-config>
		<!-- 서버의 멀티기록관 여부 (true/false) -->
		<multi>true</multi>
		<!-- 서버의 형태 (0:UNIX, 1:WINDOWS) -->
		<server-type>0</server-type>
		<!-- 네트워크 유형 (0:비폐쇄망 1:폐쇄망) -->
		<closed-network>1</closed-network>
		<!-- SSL 사용여부 (0:미사용 1:SSL사용) -->
		<ssl-type>0</ssl-type>
		<!-- 단위과제/단위업무 사용 형태 (0:모두사용,1:단위과제만사용,2:단위업무만사용) -->
		<clss-type>0</clss-type>
	</rms-config>
	
	<!-- RMS BatchWorkQueue Configuration -->
	<queue-config>
		<take-ip>10.65.206.64</take-ip>
		<take-port>7775</take-port>
		<exe-ip>10.65.206.64</exe-ip>
		<exe-port>7776</exe-port>
	</queue-config>
	
	<!-- 포맷변환서버 ip, port 설정 -->
	<formatServer-config>
		<server-ip>10.65.206.101</server-ip>
		<server-port>9100</server-port>
	</formatServer-config>
	
	<!-- RMS Report-Module Configuration -->
	<report-config>
		<!-- 레포트 형태 (0:내장모듈, 1:레포트툴-Rexpert(ClipSoft), 2:OZ(FORCS), 3:RD(m2soft)) -->
		<type>1</type>
	</report-config>
	
	<!-- RMS Origin-Module Configuration -->
	<origin-config>
		<ip>10.65.206.64</ip>
		<port>28890</port>
		<remote>true</remote>
		<view-path>/software/rmsCubrid/rms_app/download/view_temp</view-path>
	</origin-config>
	
	<!-- Files-Manage Configuration -->
	<files-config>
		<ip>10.65.206.64</ip>
		<port>28891</port>
		<scan-up-path>/software/rmsCubrid/rms_app/upload/scanctrl_temp</scan-up-path>
		<scan-down-path>/software/rmsCubrid/rms_app/download/scanctrl_temp</scan-down-path>
		<multi-up-path>/software/rmsCubrid/rms_app/upload/multictrl_temp</multi-up-path>
		<multi-down-path>/software/rmsCubrid/rms_app/download/multictrl_temp</multi-down-path>
		<up-path>/software/rmsCubrid/rms_app/upload/up_temp</up-path>
		<down-path>/software/rmsCubrid/rms_app/download/down_temp</down-path>
		<rmsstate-down-path>/software/rmsCubrid/rms_app/download/rmsstate_temp</rmsstate-down-path>
	</files-config>
	
	<!-- Storage Manage Configuration -->
	<storage-config>
		<!-- 간행물 첨부파일저장위치 -->
		<publications-storage>/software/rmsCubrid_dir/real_data/publications</publications-storage>
		<!-- 행정박물 첨부파일저장위치 -->
		<museum-storage>/software/rmsCubrid_dir/real_data/museum</museum-storage>
		<!-- 공개재분류 엑셀파일(일괄등록) 저장위치  -->
		<openresortopin-storage>/software/rmsCubrid_dir/temp/upload/openresortopin</openresortopin-storage>
		<!-- 접근범위재분류 엑셀파일 위치 -->
		<rangeresortopin-storage>/software/rmsCubrid_dir/temp/upload/rangeresortopin</rangeresortopin-storage>
		<!-- 평가폐기 엑셀파일 위치 -->
		<disuseopin-storage>/software/rmsCubrid_dir/temp/upload/rangeresortopin</disuseopin-storage>
		<!-- 매체수록 파일 저장경로 -->
		<optidisk-store-dir>/software/rmsCubrid_dir/temp/download/optidisk_store</optidisk-store-dir>
		<!-- 포맷변환 후속조치 엑셀파일 저장 위치 -->
		<formatfollowup-storage>C:/storage1/rms/temp_data/formatfollowup</formatfollowup-storage>
	</storage-config>
	
	<!-- API Manage Configuration -->
	<api-config>
		<!-- 전자문서시스템 연계시 기록관리시스템의 GR Api 정보 -->
		<group-record-ip>10.65.206.64</group-record-ip>
		<group-record-port>9512</group-record-port>
		<group-record-class>gr.classes.receiver.GRReceiveRmsImp</group-record-class>
		<group-record-lang>JAVA</group-record-lang>
		<group-record-onePlatformYn>N</group-record-onePlatformYn>
		<group-record-user>admin</group-record-user>
		<group-record-pwd>admin</group-record-pwd>
		<group-record-path>/software/rmsCubrid_dir/gr</group-record-path>
		<!-- 업무관리시스템 연계시 기록관리시스템의 대용량솔루션 정보 -->
		<business-record-ip>10.65.206.64</business-record-ip>
		<business-record-port>9510</business-record-port>
		<business-monitor-port>9610</business-monitor-port>
		<business-record-path>/software/rmsCubrid_dir/br</business-record-path>
		<!-- 중앙기록관리시스템 연계시 중앙기록관리시스템의 대용량솔루션 정보 -->
		<!-- 대용량송수신 버젼 (OLD:구버젼, NEW:표준버젼)-->
		<record-archive-type>NEW</record-archive-type>
		<record-archive-ip>10.65.206.186</record-archive-ip>
		<record-archive-port>9710</record-archive-port>
		<record-archive-path>/software/rmsCubrid_dir/ra</record-archive-path>
		<!-- 기관간인수인계 연계시 기록관리시스템 정보 -->
		<record-record-path>/software/rmsCubrid_dir/rr</record-record-path>
		<!-- 일괄등록 처리시 기록관리시스템 정보 -->
		<batch-record-path>/software/rmsCubrid_dir/batch</batch-record-path>
		<!-- 구전자문서 등록 정보 -->
		<oldelect-record-path>/software/rmsCubrid_dir/oldelect</oldelect-record-path>
		<!-- 기록관리시스템 vrms 기록물 인수 정보 -->
		<virtual-record-path>/software/rmsCubrid_dir/vr</virtual-record-path>
	</api-config>
	
	<!-- Vaccine Configuration -->
	<vaccine-config>
		<!-- Vaccine 제품구분 (1:virobot-hauri, 2:v3-ahnlab)  -->
		<make-company>1</make-company>
		<!-- Vaccine 이 설치된 서버의 bit 선택 (1:32ibt, 2:64bit) -->
		<use-bit>2</use-bit>
		<!-- Vaccine 사용여부 (true/false) -->
		<use-flag>false</use-flag>
		<!-- 기관간인수시 Vaccine 검사여부 (true/false) -->
		<take-check-flag>false</take-check-flag>
		<!-- 기관간인계시 Vaccine 검사여부 (true/false) -->
		<takovr-check-flag>false</takovr-check-flag>
		
		<!-- 이관 Vaccine 치료파일 경로 -->
		<dftrans-clean-path>/software/rmsCubrid_daemon/orign/clean/dftrans</dftrans-clean-path>
		<!-- 인수 Vaccine 치료파일 경로 -->
		<take-clean-path>/software/rmsCubrid_daemon/orign/clean/take</take-clean-path>
		<!-- 인수 Vaccine 치료대상 임시 경로 -->
		<take-clean-temp-path>/software/rmsCubrid_daemon/orign/cleanTemp/take</take-clean-temp-path>
		<!-- 보존 Vaccine 치료파일 경로 -->
		<presv-clean-path>/software/rmsCubrid_daemon/orign/clean/presv</presv-clean-path>
		
		<!-- 이관 Vaccine검사요청 분할 카운트(client용) -->
		<dftrans-scan-req-cnt>1</dftrans-scan-req-cnt>
	</vaccine-config>
	
	<!-- Search Configuration -->
	<search-config>
		<!-- 검색화면 열람범위 관리유형(0:열람불가 목록숨기기, 1:열람불가  목록보기) -->
		<read-range-type>1</read-range-type>
		<!-- 생산부서 사용자 접근유형(0:접근불가, 1:접근허용) -->
		<org-user-accs-type>0</org-user-accs-type>
		<!-- 검색 엔진 정보 -->
		<ip>10.65.206.170</ip>
		<port>7578</port>
		<cgi-name>srch_resultxml</cgi-name>
	</search-config>
	
	<!-- BRM/GCC-Receve Configuration -->
	<brm-gcc-config>
		<!-- BRM 연동 Home Directory -->
		<ifbrm-home>/software/rmsCubrid_daemon/ifbrm</ifbrm-home>
		<!-- BRM 보존기간 전달 저장 path -->
		<brm-send-path>/software/rmsCubrid_daemon/mogaha/snd_brm</brm-send-path>
		<brm-path>/software/rmsCubrid_daemon/mogaha/rcv_brm</brm-path>
		<gcc-path>/software/rmsCubrid_daemon/mogaha/rcv_gcc</gcc-path>
	</brm-gcc-config>
	
	<!-- Create System dependent Menu Configuration -->
	<!-- 각 생산시스템에 종속적인 메뉴(메뉴ID)를 세팅한다. 전자문서(edms) / 업무관리(bms) / 통합(unity) -->
	<menu-config>
		<!-- 전자문서 : 생산현황접수(69) / 전자문서시스템생산접수(107) / 전자문서시스템접수현황(215) -->
		<edms>69|107|215</edms>
		<bms></bms>
		<unity></unity>
	</menu-config>
	
	<!-- 통합정보공개 시스템 연계  -->
	<openlist-config>
		<!--  웹서비스(건) 전송파일 위치-->
		<send-path>/software/rmsCubrid_dir/webservice/sendfile</send-path>
		<!--  웹서비스(건) keystore alias passwd -->
		<passwd>open2005</passwd>
	</openlist-config>
	<opensearchlist-config>
		<!--  웹서비스(철) 전송파일 위치-->
		<send-path>/software/rmsCubrid_dir/webservice/sendfile</send-path>
		<!--  웹서비스(철) keystore alias passwd -->
		<passwd>open2005</passwd>
	</opensearchlist-config>
	
	<!-- rms Sso INCODEING Configuration -->
	<sso-config>
		<!-- SSO 인코딩 형태 (all : 전체 Parameter 인코딩, ui:Parameter 중에서 UI 만 인코딩) -->
		<!-- (mogaha : 행정자치부), (cha : 문화재청), (kcg : 해양경찰청) -->
		<sso-type>bms</sso-type>
		<!-- 보안 로그인 유형 (0:미사용, 1:GPKI 표준 API) -->
		<cert-type>0</cert-type>
		<!-- DRM 유형 (0:미사용, 1:표준JSP호출) -->
		<drm-type>0</drm-type>
		<!-- DRM Client 유형 (0:미사용, 1:fasso 클라이언트) -->
		<drm-client-type>0</drm-client-type>
	</sso-config>
	
	<!-- 사용자 패스워드관리 설정 -->
	<user-pw-config>
		<!-- 사용자 패스워드 변경 제한 일자 (0:제한없음) -->
		<limit-day-change-pw>0</limit-day-change-pw>
		<!-- 사용자 패스워드 입력실패 제한 횟수 (0:제한없음) -->
		<limit-cnt-fault-pw>0</limit-cnt-fault-pw>
	</user-pw-config>
	
	<!-- 기관코드 설정 -->
	<org-config>
		<!-- 1:전체기관명, 2:기관명(대표기관명제외), 3:기관약칭 -->
		<org-nm-div>2</org-nm-div>
	</org-config>
	
	<!--  포맷 변환 버전 설정 -->
	<format-config>
		<!-- OLD : 변경이전, NEW: 변경후-->
		<format-type>NEW</format-type>
	</format-config>
	
	<!-- 일괄등록설정 -->
	<batch-reg-config>
		<!--행정DB일괄등록시 단위업무 지원 유형 (0:구기록물 단위업무 '99999999'만 허용, 1:신규 단위업무 지원)-->
		<new-unit-div>0</new-unit-div>
	</batch-reg-config>
	
	<!-- 평가폐기 환경설정 -->
	<eval-config>
		<!-- 폐기 : 30년이상 기록물 폐기대상 생성구분 (0: 해당없음, 1:대상) -->
		<disuse-presv-30year-div>1</disuse-presv-30year-div>
	</eval-config>
	
	<!-- 분류기준표(단위업무) 관리 설정  -->
	<unit-config>
		<!-- 분류기준표 관리 유형 (0:기록관자체관리, 1:CORA연계) -->
		<unit-mng-type>0</unit-mng-type>
		<!-- 분류기준표 연계파일 위치정보 -->
		<unit-path>/software/rmsCubrid_dir/ra</unit-path>
		<!-- 분류기준표 prefix(대/중/소) 설정 -->
		<pk-lrg-prefix>A</pk-lrg-prefix>
		<pk-mid-prefix>B</pk-mid-prefix>
		<pk-sml-prefix>C</pk-sml-prefix>
		<!-- 기관고유 단위업무 prefix -->
		<pk-unit>RM</pk-unit>
	</unit-config>

	<!-- 공개재분류 환경설정 -->
	<open-config>
		<!-- 공개전환 구분 : 공개전환 기간 (0: 자동공개, 1: 15일, 2:20일, 3:30일) -->
		<open-resort-auto-div>1</open-resort-auto-div>
	</open-config>	
</configuration>