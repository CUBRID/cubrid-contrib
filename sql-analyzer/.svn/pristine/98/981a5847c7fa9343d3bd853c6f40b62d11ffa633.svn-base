<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE properties SYSTEM "http://java.sun.com/dtd/properties.dtd">
<properties>
<comment>Config for Stand Alone Daemon</comment>

<!-- sendlist 파싱 디렉토리 -->
<entry key="send-list-br">C:/software/rms_dir/br</entry>

<!-- 업무관리시스템으로부터 접수받은 파일목록을 읽는 작업 실행 (on/off) -->
<entry key="recv-file-list">on</entry>
<!-- 업무관리시스템으로부터 접수받은 파일목록을 읽는 작업 실행 주기 (초) -->
<entry key="recv-file-list-period">5</entry>

<!-- 업무관리시스템 접수작업 실행 (on/off) -->
<entry key="br-take">on</entry>
<!-- 업무관리시스템 접수작업 실행 주기 (초) -->
<entry key="br-take-period">600</entry>
<!-- 업무관리시스템 접수작업 실행 최대 쓰레드 갯수 -->
<entry key="br-take-thread-cnt">3</entry>
<!-- 업무관리시스템 접수작업큐 등록 실행 주기 (초) -->
<entry key="br-take-queue-period">1000</entry>

<!-- 대용량송수신 버젼 (OLD:구버젼, NEW:표준버젼) -->
<entry key="record-archive-type">OLD</entry>
</properties>