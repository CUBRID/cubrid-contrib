-- Query Id : updateDisuseFinalRsltRegDocNo

		UPDATE TB_DFDISUSEOPIN A		SET A.REVIEW_COFIRM_CD = '1'		    , A.REVIEW_DOC_NO = '1'		    , A.DISCU_PROG_STATE_CD = '09'		WHERE A.RECORD_CENTER_ID = '1'		AND A.DISCU_YYYY = '1'		AND A.DISCU_YYYY_SEQ = '1'		AND A.DISCU_PROG_STATE_CD IN ('07','08','09')		AND ((A.DISCU_REQ_FLAG = '0' AND A.DISCU_PROG_STATE_CD IN ('07','09'))			OR (A.DISCU_REQ_FLAG = '1' AND A.DISCU_PROG_STATE_CD IN ('08','09'))		)   		AND A.REVIEW_COFIRM_CD = '0'		AND EXISTS (		    SELECT 1 		    FROM TB_RDFOLDER B 		    WHERE B.RECORD_CENTER_ID = A.RECORD_CENTER_ID 		    AND B.FOLDER_ID = A.FOLDER_ID 		     AND  					B.PRESV_TERM_CD in ('01', '03', '05', '10', '25')				  		)	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\eval\disuseeval\DisuseFinalRslt.xml
-- Query    : insert
-- Query Id : createDisuseFinalRsltRegHist

		INSERT INTO TB_DFDISUSEOPINHIST			SELECT A.RECORD_CENTER_ID		    	, A.FOLDER_ID		      	, ( SELECT NVL(MAX(CHNG_HIST_NO),0) + 1 FROM TB_DFDISUSEOPINHIST WHERE RECORD_CENTER_ID = A.RECORD_CENTER_ID AND FOLDER_ID = A.FOLDER_ID ) AS CHNG_HIST_NO		      	, A.DISCU_YYYY		      	, CASE WHEN A.DISCU_REQ_FLAG = '1' THEN A.NAT_CMT_DIV_CD ELSE A.REVIEW_DIV_CD END AS REVIEW_OPIN		      	, A.CUR_PRESV_TERM_CD		      	, A.REVIEW_REFIX_AFT_PRESV_TERM AS REVIEW_PRESV_TERM_CD		      	, CASE WHEN A.DISCU_REQ_FLAG = '1' THEN A.NAT_CMT_DEFER_CD ELSE A.REVIEW_DEFER_CD END AS REVIEW_DEFER_CD		      	, CASE WHEN A.DISCU_REQ_FLAG = '1' THEN A.NAT_CMT_REFIX_AFT_RSN ELSE A.REVIEW_REFIX_AFT_RSN END AS REVIEW_RSN		      	, CASE WHEN A.DISCU_REQ_FLAG = '1' THEN A.NAT_CMT_YMD ELSE A.REVIEW_YMD END AS REVIEW_YMD		      	, A.DISCU_YYYY_SEQ AS DISCU_SEQ 		      	, '1' AS RECORD_CHRGR_ID		   	FROM TB_DFDISUSEOPIN A, TB_RDFOLDER B , TB_DFDISUSEDISCUDOC C		 	WHERE A.RECORD_CENTER_ID = '1'		   	AND A.DISCU_YYYY = '1'		   	AND A.DISCU_PROG_STATE_CD = '09'		   	AND A.DISCU_YYYY_SEQ = '1'		   	AND A.REVIEW_DOC_NO IS NOT NULL		   	AND A.REVIEW_COFIRM_CD = '1' AND  					B.PRESV_TERM_CD in ('01', '03', '05', '10', '25')			   	  		   	AND B.RECORD_CENTER_ID = A.RECORD_CENTER_ID		   	AND B.FOLDER_ID        = A.FOLDER_ID		   	AND C.RECORD_CENTER_ID = A.RECORD_CENTER_ID		   	AND C.DISCU_YYYY       = A.DISCU_YYYY		   	AND C.DISCU_YYYY_SEQ   = A.DISCU_YYYY_SEQ	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\eval\disuseeval\DisuseFinalRslt.xml
-- Query    : update
-- Query Id : updateDisuseFinalRsltRegFolder1

		UPDATE TB_RDFOLDER A		SET A.DISUSE_STATE_CD = '09'		    , A.LAST_EVAL_YYYY = '1'		    , A.DISU_DIV_CD = ( SELECT CASE WHEN B.DISCU_REQ_FLAG = '1' THEN B.NAT_CMT_DIV_CD ELSE B.REVIEW_DIV_CD END FROM TB_DFDISUSEOPIN B WHERE B.RECORD_CENTER_ID = A.RECORD_CENTER_ID AND B.FOLDER_ID = A.FOLDER_ID AND B.DISCU_YYYY = '1' )		WHERE A.RECORD_CENTER_ID = '1'		AND A.DISUSE_EVAL_PROG_FLAG = '1'		AND EXISTS (			SELECT C.FOLDER_ID		    FROM TB_DFDISUSEOPIN C		    WHERE C.RECORD_CENTER_ID = A.RECORD_CENTER_ID		    AND C.FOLDER_ID = A.FOLDER_ID		    AND C.DISCU_YYYY = '1'		    AND C.DISCU_PROG_STATE_CD = '09'		    AND C.DISCU_YYYY_SEQ = '1'		    AND C.REVIEW_DOC_NO IS NOT NULL		    AND C.REVIEW_COFIRM_CD = '1'		)	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\eval\disuseeval\DisuseFinalRslt.xml
-- Query    : update
-- Query Id : updateDisuseFinalRsltRegFolder2

      UPDATE TB_RDFOLDER A            SET  A.disu_delay_cd = (CASE WHEN A.disu_div_cd = '2' THEN (CASE WHEN (SELECT B.discu_req_flag FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') = '1' THEN (SELECT B.nat_cmt_defer_cd FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1')      			                                            ELSE (SELECT B.review_defer_cd FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') END)      			        					ELSE A.disu_delay_cd END)      		    , A.presv_term_cd = NVL((CASE WHEN A.disu_div_cd = '3' THEN (CASE WHEN (SELECT B.discu_req_flag FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') = '1' THEN A.presv_term_cd      														ELSE (SELECT B.review_refix_aft_presv_term FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') END)      					ELSE A.presv_term_cd END),A.presv_term_cd)      		    , A.expt_disuse_yyyy = (CASE WHEN TO_NUMBER((CASE WHEN A.disu_div_cd = '3' THEN (CASE WHEN (SELECT B.discu_req_flag FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') = '1' THEN A.presv_term_cd      														ELSE (SELECT B.review_refix_aft_presv_term FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') END)      					ELSE A.presv_term_cd END)) >= 20 THEN ''      		                                 ELSE TO_CHAR(TO_NUMBER((CASE WHEN A.creat_sys_cd IN ('01','04') THEN A.creat_yyyy ELSE NVL(SUBSTR(A.folder_close_dtime,0,4),A.creat_yyyy) END) )+TO_NUMBER((CASE WHEN A.disu_div_cd = '3' THEN (CASE WHEN (SELECT B.discu_req_flag FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') = '1' THEN A.presv_term_cd      														ELSE (SELECT B.review_refix_aft_presv_term FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') END)      					ELSE A.presv_term_cd END))) END)      		    , A.expt_transf_yyyy = (CASE WHEN TO_NUMBER((CASE WHEN A.disu_div_cd = '3' THEN (CASE WHEN (SELECT B.discu_req_flag FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') = '1' THEN A.presv_term_cd      														ELSE (SELECT B.review_refix_aft_presv_term FROM TB_DFDISUSEOPIN B WHERE B.record_center_id=A.record_center_id AND B.folder_id=A.folder_id AND B.discu_yyyy='1') END)      					ELSE A.presv_term_cd END)) < 20 THEN ''      		                                 ELSE (CASE WHEN A.folder_type_div_cd IN ('3','4') THEN TO_CHAR(TO_NUMBER( (CASE WHEN A.creat_sys_cd IN ('01','04') THEN A.creat_yyyy ELSE NVL(SUBSTR(A.folder_close_dtime,0,4),A.creat_yyyy) END))+5) ELSE TO_CHAR(TO_NUMBER( (CASE WHEN A.creat_sys_cd IN ('01','04') THEN A.creat_yyyy ELSE NVL(SUBSTR(A.folder_close_dtime,0,4),A.creat_yyyy) END))+10) END) END)      		    , A.disuse_eval_prog_flag = '0'      WHERE A.record_center_id = '1'         AND   A.disuse_eval_prog_flag = '1'         AND   A.disuse_state_cd = '09'         AND   A.last_eval_yyyy = '1'         AND EXISTS (      			    SELECT C.folder_id      			    FROM TB_DFDISUSEOPIN C      			    WHERE C.record_center_id = A.record_center_id      			    AND C.folder_id = A.folder_id      			    AND C.discu_yyyy = '1'      			    AND C.discu_prog_state_cd = '09'      			    AND C.discu_yyyy_seq = '1'      			    AND C.review_doc_no IS NOT NULL      			    AND C.review_cofirm_cd = '1'      );


