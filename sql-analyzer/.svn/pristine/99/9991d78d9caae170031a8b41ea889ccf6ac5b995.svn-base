-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\eval\disuseeval\DisuseFinalRslt.xml
-- Query    : select
-- Query Id : findDisuseFinalRsltRegPagedList

		SELECT E.*		FROM (			SELECT ROWNUM AS rnum				, D.*			FROM (				SELECT A.DISCU_YYYY_SEQ				    , MAX(C.DISCU_YYYY || '-' || LPAD(C.DISCU_YYYY_SEQ,6,'0')) AS DISCU_YYYY_SEQ_NO				    , MAX(C.DISCU_CHOICE_RSN) AS DISCU_CHOICE_RSN				    , MAX(C.DISCU_REQ_FLAG) AS DISCU_REQ_FLAG				    , MAX((SELECT COM_CD_NM FROM TB_ZZCOMCD WHERE COM_TYPE_CD = 'DF14' AND COM_CD = C.DISCU_REQ_FLAG)) AS DISCU_REQ_FLAG_NM     				    , COUNT(*) AS TOT_CNT1				    , SUM(CASE WHEN A.REVIEW_COFIRM_CD = '0' THEN 1 ELSE 0 END) AS OPIN1_CNT1				    , SUM(CASE WHEN A.REVIEW_COFIRM_CD = '1' AND ( (A.DISCU_REQ_FLAG='0' AND A.REVIEW_DIV_CD = '1') OR (A.DISCU_REQ_FLAG='1' AND A.NAT_CMT_DIV_CD = '1') ) THEN 1 ELSE 0 END) AS OPIN2_CNT1				    , SUM(CASE WHEN A.REVIEW_COFIRM_CD = '1' AND ( (A.DISCU_REQ_FLAG='0' AND A.REVIEW_DIV_CD = '2') OR (A.DISCU_REQ_FLAG='1' AND A.NAT_CMT_DIV_CD = '2') ) THEN 1 ELSE 0 END) AS OPIN3_CNT1				    , SUM(CASE WHEN A.REVIEW_COFIRM_CD = '1' AND ( (A.DISCU_REQ_FLAG='0' AND A.REVIEW_DIV_CD = '3') OR (A.DISCU_REQ_FLAG='1' AND A.NAT_CMT_DIV_CD = '3') ) THEN 1 ELSE 0 END) AS OPIN4_CNT1				    , MAX(A.REVIEW_DOC_NO) AS REVIEW_DOC_NO				    , MAX(C.CONFIRM_FLAG) AS CONFIRM_FLAG				    , MAX(A.REVIEW_COFIRM_CD) AS REVIEW_COFIRM_CD				    , SUM(CASE WHEN A.REVIEW_COFIRM_CD = '0' AND A.DISCU_PROG_STATE_CD <= '06' THEN 1 ELSE 0 END) AS REVIEW_NOT_CNT				FROM TB_DFDISUSEOPIN A, TB_RDFOLDER B, TB_DFDISUSEDISCUDOC C				WHERE A.RECORD_CENTER_ID = '1'				AND A.DISCU_YYYY = '1'				AND A.DISCU_PROG_STATE_CD IN ('06','07','08','09')				AND A.DISCU_YYYY_SEQ IS NOT NULL				AND B.RECORD_CENTER_ID = A.RECORD_CENTER_ID				AND B.FOLDER_ID        = A.FOLDER_ID				AND C.RECORD_CENTER_ID = A.RECORD_CENTER_ID				AND C.DISCU_YYYY       = A.DISCU_YYYY				AND C.DISCU_YYYY_SEQ   = A.DISCU_YYYY_SEQ 				GROUP BY A.DISCU_YYYY_SEQ			) D			WHERE ROWNUM <= '1'		) E		WHERE E.rnum >= '1'	;


