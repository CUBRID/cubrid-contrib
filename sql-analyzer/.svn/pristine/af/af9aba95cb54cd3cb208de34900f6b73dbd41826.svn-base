<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMapConfig PUBLIC "-//ibatis.apache.org//DTD SQL Map Config 2.0//EN" "http://ibatis.apache.org/dtd/sql-map-config-2.dtd">

<sqlMapConfig>
	<settings 
		cacheModelsEnabled="true" 
		enhancementEnabled="true"
		lazyLoadingEnabled="true" 
		maxRequests="128" 
		maxSessions="10"
		maxTransactions="5" 
		useStatementNamespaces="true"
		defaultStatementTimeout="1000"
	/>

	<!-- 7.srch -->
	<!-- 7.1.srch -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/ConditionalSrch.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/FtrSrch.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/ClssSrch.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/OrgTakeTrgtSrch.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/PublicationsSrch.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/srch/MuseumSrch.xml" />
	
	<!-- 7.2.read -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/read/ReadReq.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/read/Read.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/read/ReadHandl.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/read/ReadAppr.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/read/ReadStat.xml" />
	<!-- 7.3.stat -->
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/CreateCurState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/PresvCurState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/DisuseCurState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/TransfCurState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/OpenCurState.xml" />
	<sqlMap resource="egovframework/sqlmap/rms/default/rms/srch/stat/UsageCurState.xml" />

</sqlMapConfig>