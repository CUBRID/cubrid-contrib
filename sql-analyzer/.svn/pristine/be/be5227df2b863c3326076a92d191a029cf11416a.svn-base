		                       ,#regDtime:VARCHAR#,#attachTypeCd:VARCHAR#
		                       ,#mediaFormatEtc:VARCHAR#,#mediumEtc:VARCHAR#
		                       ,#recordSno1:VARCHAR#,#attachEtc:VARCHAR#, #recordClssCd:VARCHAR#, #lineNo:VARCHAR#)
	</insert>
	
	<insert id="creatOldElectRecordCreatReg" parameterClass="rms.take.oldElectReg.domain.RecordCreat">
    INSERT INTO TB_TKRECORDCREAT (record_center_id,record_take_seq
                                 ,agent_sno,agent_type_cd
                                 ,agent_nm,agent_org_cd
                                 ,agent_org_nm,duty_type_cd
                                 ,duty_act_dtime,reg_dtime)
			             VALUES (#recordCenterId:VARCHAR#,#recordTakeSeq:VARCHAR#
			                    ,(SELECT NVL(MAX(AGENT_SNO),0)+1 AGENT_SNO
			                        FROM TB_TKRECORDCREAT
			                       WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#
			                         AND RECORD_TAKE_SEQ  = #recordTakeSeq:VARCHAR#),#agentTypeCd:VARCHAR#
			                    ,#agentNm:VARCHAR#,#agentOrgCd:VARCHAR#
			                    ,#agentOrgNm:VARCHAR#,#dutyTypeCd:VARCHAR#
				                ,#dutyActDtime:VARCHAR#,#regDtime:VARCHAR#)
	</insert>
	<update id="updateTkFolderErecordFlag" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg">
		update TB_TKFOLDER 
		   set erecord_flag = (select sum(erecord_flag) 
		   						 from ( select distinct erecord_flag 
		   						 		  from TB_TKRECORD
		   						 		 where record_center_id = #recordCenterId:VARCHAR#
		   						 		   and creat_sys_cd = #creatSysCd:VARCHAR#
		   						 		   and folder_take_seq = #folderTakeSeq#
		   						 		   and reg_dtime = #regDtime:VARCHAR#
    							))
		 where record_center_id = #recordCenterId:VARCHAR#
		   and creat_sys_cd = #creatSysCd:VARCHAR#
		   and folder_take_seq = #folderTakeSeq:VARCHAR#
		   and reg_dtime = #regDtime:VARCHAR#
	</update>
	
	<!-- 첨부파일 등록 -->
	<insert id="creatOldElectAttachFileReg" parameterClass="rms.take.oldElectReg.domain.AttachFileOldElect">
	    INSERT INTO TB_TKATTACHFILE (record_center_id
	    							,record_take_seq
	                                ,reg_seq,org_cd
	                                ,creat_yyyy
	                                ,creat_reg_no
	                                ,attach_file_seq
	                                ,attach_file_page_cnt
	                                ,doc_type_cd
	                                ,doc_type_sno
	                                ,file_nm
	                                ,file_size
	                                ,attach_file_path
	                                ,attach_file_type
	                                ,reg_dtime)
				             VALUES(#recordCenterId:VARCHAR#
				             		,#recordTakeSeq:VARCHAR#
				                    , (SELECT NVL(MAX(REG_SEQ)+1, 1) 
				                    	 FROM TB_TKATTACHFILE 
				                    	WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
				                    	  AND RECORD_TAKE_SEQ = #recordTakeSeq:VARCHAR# 
				                    	  AND ATTACH_FILE_TYPE = #attachFileType:VARCHAR#)
				                    ,#orgCd:VARCHAR#
				                    ,#creatYyyy:VARCHAR#
				                    ,#creatRegNo:VARCHAR#
				                    ,#attachFileSeq:VARCHAR#
				                    ,#attachFilePageCnt:VARCHAR#
				                    ,#docTypeCd:VARCHAR#
				                    ,#docTypeSno:VARCHAR#
				                    ,#fileNm:VARCHAR#
				                    ,#fileSize:VARCHAR#
				                    ,#attachFilePath:VARCHAR#
				                    ,#attachFileType:VARCHAR#
				                    ,#regDtime:VARCHAR#)
	</insert>

	
	<!-- 건별 기록물철인수일련번호 조회 -->
	<select id="findOldElectTakeSeq" parameterClass="rms.take.oldElectReg.domain.FolderOldElectReg" resultClass="string">
		SELECT /*+INDEX(TB_TKFOLDER TB_TKFOLDER_UX01)*/FOLDER_TAKE_SEQ
		FROM TB_TKFOLDER
		WHERE CREAT_SYS_CD = '06'
		  AND ORG_CD = #orgCd:VARCHAR#
		  AND CREAT_YYYY = #creatYyyy:VARCHAR#
		  AND CLSS_ID = #clssId:VARCHAR#
		  AND FOLDER_REG_SEQ = #folderRegSeq:VARCHAR#
		  AND HO_NO = #hoNo:VARCHAR# 
	</select>

	<!-- 기록물철 일련번호 중복 수 조회 -->
	<select id="findOldElectOrgInfo" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultMap="findOldElectOrgInfo-result">
		SELECT ORG_NM
		  FROM TB_STORG
		 WHERE record_center_id=#recordCenterId:VARCHAR#
		   AND org_cd =#orgCd:VARCHAR#
	</select>
	
	<select id="findRecordOldElectTakeSeq" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultClass="string">
	    SELECT TO_CHAR(SYSDATE,'YYYY')||LPAD((tb_tkrecord_sq01.nextval), 15, '0') record_take_seq 
	      FROM DUAL
	</select>
	<!-- 기록물철 분류번호 중복 체크  -->
	<select id="isOldElectTkFolder" parameterClass="rms.take.oldElectReg.domain.FolderOldElectReg" resultClass="string">
		SELECT 'O'
		  FROM TB_TKFOLDER
		 WHERE RECORD_CENTER_ID=#recordCenterId:VARCHAR#
		   AND CREAT_SYS_CD='06'
		   AND ORG_CD=#orgCd:VARCHAR# 
		   AND CREAT_YYYY=#creatYyyy:VARCHAR#
		   AND CLSS_ID=#clssId:VARCHAR#
		   AND FOLDER_REG_SEQ=#folderRegSeq:VARCHAR#
		   AND HO_NO=#hoNo:VARCHAR#
	</select>
	<select id="isOldElectTkRecord" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultClass="string">
		SELECT 'O'
		  FROM TB_TKRECORD
		 WHERE RECORD_CENTER_ID	= #recordCenterId:VARCHAR#
		   AND CREAT_SYS_CD		= #creatSysCd:VARCHAR#
		   AND ORG_CD			= #orgCd:VARCHAR# 
		   AND CREAT_YYYY		= #creatYyyy:VARCHAR#
		   AND CREAT_REG_NO		= #creatRegNo:VARCHAR#
		   AND DTCH_REG_NO		= #dtchRegNo:VARCHAR#
		   AND FOLDER_CLSS_NO 	= #folderClssNo:VARCHAR#
		   <dynamic>
		   	<isNotEmpty prepend="AND" property="creatRegDtime">
		   		CREAT_REG_DTIME = #creatRegDtime:VARCHAR#
		   	</isNotEmpty>
		   </dynamic>
		   and rownum &lt; 2
	</select>
	<select id="findOldElectRecordTakeSeq" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultClass="string">
		select record_take_seq
		  from TB_TKRECORD
		 where record_center_id 	= #recordCenterId:VARCHAR#
		   and creat_sys_cd 		= #creatSysCd:VARCHAR#
		   and org_cd 				= #orgCd:VARCHAR# 
		   and creat_reg_dtime 		= #creatRegDtime:VARCHAR#
		   and creat_reg_no			= #creatRegNo:VARCHAR#
		   and dtch_reg_no 			= #dtchRegNo:VARCHAR#
		   <dynamic>
		   	<isNotEmpty property="regDtime" prepend="and">
		   		reg_dtime 			= #regDtime:VARCHAR#
		   	</isNotEmpty>
		   </dynamic>
	</select>
	
	<!-- 기록물건 등록 순번 -->
	<select id="getRecordTakeSeq" parameterClass="string" resultClass="string">
		select nvl(max(record_sno), '1') 
		  from TB_TKRECORD A
		 where A.record_take_seq = #string:VARCHAR#
	</select>
	
	<!-- 기록물건등록번호조회(파일정보) -->
	<select id="findOldElectRegRecordTakeSeq" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultClass="string">
			SELECT RECORD_TAKE_SEQ
			  FROM TB_TKRECORD
		     WHERE record_center_id  = #recordCenterId:VARCHAR#
               AND creat_sys_cd      = #creatSysCd:VARCHAR#
               AND org_cd            = #orgCd:VARCHAR#
               AND creat_reg_dtime   = #creatRegDtime:VARCHAR#
               AND creat_reg_no      = #creatRegNo:VARCHAR#
               AND dtch_reg_no       = #dtchRegNo:VARCHAR#
                    
	</select>
	<update id="updateOldElectStateFlag" parameterClass="rms.take.oldElectReg.domain.FolderOldElectReg">
		UPDATE TB_TKRECVHIST
		SET state_flag = #stateFlag:VARCHAR#
		WHERE record_center_id=#recordCenterId:VARCHAR# and recv_dtime=#recvDtime:VARCHAR# and creat_sys_cd=#creatSysCd:VARCHAR# 
	</update>
	
	
	<!-- 삭제 Set -->
	
	
	
	<delete id="deleteOldElectAttachFile" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		DELETE 
		  FROM TB_TKATTACHFILE
		 WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
		   AND RECORD_TAKE_SEQ IN (SELECT RECORD_TAKE_SEQ
		                             FROM TB_TKRECORD
		                            WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
		                              AND FOLDER_TAKE_SEQ IN (SELECT FOLDER_TAKE_SEQ
		                                                        FROM TB_TKFOLDER
		                                                       WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#  
		                                                         AND CREAT_SYS_CD = #creatSysCd:VARCHAR#
		                                                         AND REG_DTIME = #regDtime:VARCHAR#))
	</delete>
	
	<delete id="deleteOldElectRecordCreat" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		DELETE 
		  FROM TB_TKRECORDCREAT
		 WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
		   AND RECORD_TAKE_SEQ IN (SELECT RECORD_TAKE_SEQ
		                             FROM TB_TKRECORD
		                            WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
		                              AND FOLDER_TAKE_SEQ IN (SELECT FOLDER_TAKE_SEQ
		                                                        FROM TB_TKFOLDER
		                                                       WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#  
		                                                         AND CREAT_SYS_CD = #creatSysCd:VARCHAR#
		                                                         AND REG_DTIME = #regDtime:VARCHAR#))
	</delete>
	<delete id="deleteOldElectRecord" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		DELETE
          FROM TB_TKRECORD
         WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
           AND FOLDER_TAKE_SEQ IN (SELECT FOLDER_TAKE_SEQ
                                     FROM TB_TKFOLDER
                                    WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#  
                                      AND CREAT_SYS_CD = #creatSysCd:VARCHAR#
                                      AND REG_DTIME = #regDtime:VARCHAR#)
	</delete>
	<delete id="deleteOldElectFolderCreat" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		DELETE
          FROM TB_TKFOLDERCREAT
         WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR# 
           AND FOLDER_TAKE_SEQ IN (SELECT FOLDER_TAKE_SEQ
                                     FROM TB_TKFOLDER
                                    WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#  
                                      AND CREAT_SYS_CD = #creatSysCd:VARCHAR#
                                      AND REG_DTIME = #regDtime:VARCHAR#)
	</delete>
	<delete id="deleteOldElectFolderTakeTakovr" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		delete
		from tb_tkfoldertaketakovr
		where reg_dtime = #regDtime:VARCHAR# 
		and record_center_id=#recordCenterId:VARCHAR#
	</delete>
	<delete id="deleteOldElectFolder" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		DELETE
          FROM TB_TKFOLDER
         WHERE RECORD_CENTER_ID = #recordCenterId:VARCHAR#  
           AND CREAT_SYS_CD = #creatSysCd:VARCHAR#
           AND REG_DTIME = #regDtime:VARCHAR#
	</delete>
	<delete id="deleteOldElectOrgPlan" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		delete
		  from tb_tkorgtakeplan
		 where record_center_id=#recordCenterId:VARCHAR# 
		   and creat_sys_cd = #creatSysCd:VARCHAR#
		   and reg_dtime = #regDtime:VARCHAR#
	</delete>
	<delete id="deleteOldElectRecvHist" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		delete
		  from tb_tkrecvhist
		 where record_center_id=#recordCenterId:VARCHAR# 
	   	   and creat_sys_cd = #creatSysCd:VARCHAR#
		   and recv_dtime = #regDtime:VARCHAR#
	</delete>
	<delete id="deleteOldElectRecvError" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile">
		delete
		  from tb_tkrecverror
		 where record_center_id=#recordCenterId:VARCHAR# 
		   and creat_sys_cd = #creatSysCd:VARCHAR#
		   and recv_dtime = #regDtime:VARCHAR#
	</delete>
	
	
	
	<resultMap id="findOldElectRegInfo-result" class="java.util.HashMap">
		<result property="recordCenterId" column="RECORD_CENTER_ID"/>
		<result property="recvDtime" column="RECV_DTIME"/>
		<result property="takeReqUserId" column="TAKE_REQ_USER_ID"/>
		<result property="takeReqUserNm" column="TAKE_REQ_USER_NM"/>
		<result property="takeOrgCd" column="TAKE_ORG_CD"/>
		<result property="takeOrgNm" column="TAKE_ORG_NM"/>
	</resultMap>
	<select id="findOldElectRegInfo" parameterClass="java.util.HashMap" resultMap="findOldElectRegInfo-result">
		select   record_center_id
			   , reg_dtime recv_dtime
			   , take_req_user_id
			   , take_req_user_nm
			   , take_org_cd
			   , (select org_nm from TB_STORG where org_cd = take_org_cd) take_org_nm
		  from   TB_TKORGTAKEPLAN
		 where 	 creat_sys_cd 	= #creatSysCd:VARCHAR#
		   and 	 take_yyyy 		= #creatYyyy:VARCHAR#
		   and 	 take_org_cd 	= #takeOrgCd:VARCHAR#
		   and 	 take_sno 		= #takeSno:NUMBER#
	</select>
	<resultMap id="oldElectErrorHist-result" 				class="rms.take.oldElectReg.domain.OldElectRegFile">
		<result property="lineNo"	 		    column="LINE_NO"/>
		<result property="recvErrorRsn"	 		column="RECV_ERROR_RSN"/>
		<result property="fileDivNm"	 		column="FILE_DIV_NM"/>
	</resultMap>
	<!-- 오류내역조회 -->
	<select id="findOldElectRegErrorList" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile" resultMap="oldElectErrorHist-result">
	SELECT C.*
    FROM (
        SELECT ROWNUM AS rnum
            , B.*
        FROM (
			select    a.line_no
					, a.recv_error_rsn
					, (SELECT COM_CD_NM FROM TB_ZZCOMCD WHERE COM_TYPE_CD = 'TK10' AND COM_CD = file_div_cd) file_div_nm
			  from tb_tkrecverror a
			 where record_center_id 	= #recordCenterId:VARCHAR# 
			   and recv_dtime			= #recvDtime:VARCHAR#
			 <dynamic>
			 	<isNotEmpty prepend="AND" property="fileDivCd">
			 		a.file_div_cd in ($fileDivCd$)
			 	</isNotEmpty>
			 </dynamic>
			 order by file_div_cd asc, to_number(decode(line_no, 'Exception', 0, line_no))
			 ) B
        WHERE ROWNUM <![CDATA[<=]]> #pagedEnd:NUMBER#
    ) C
    WHERE C.rnum <![CDATA[>=]]> #pagedStart:NUMBER# 
	</select>
	<select id="countOldElectRegErrorList" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile" resultClass="long">
			select count(1)
			  from tb_tkrecverror a
			 where record_center_id = #recordCenterId:VARCHAR# 
			   and recv_dtime=#recvDtime:VARCHAR#
			<dynamic>
			 	<isNotEmpty prepend="AND" property="fileDivCd">
			 		a.file_div_cd in ($fileDivCd$)
			 	</isNotEmpty>
			 </dynamic>	
	</select>
	
	<select id="findErecordFlag" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultClass="string">
		select erecord_flag 
		  from TB_TKRECORD 
		 where record_center_id 	= #recordCenterId:VARCHAR#
		   and org_cd 				= #orgCd:VARCHAR# 
		   and creat_sys_cd 		= #creatSysCd:VARCHAR#
		   and creat_reg_dtime 		= #creatRegDtime:VARCHAR#
		   and creat_reg_no			= #creatRegNo:VARCHAR#
		   and dtch_reg_no 			= #dtchRegNo:VARCHAR#
		   <dynamic>
		   	<isNotEmpty property="regDtime" prepend="and">
		   		reg_dtime 			= #regDtime:VARCHAR#
		   	</isNotEmpty>
		   </dynamic>
	</select>
	
	<resultMap id="findErrorRecordReg-result" class="java.util.HashMap">
		<result property="lineNo" 			column="LINE_NO"/>
		<result property="recordRegCnt" 	column="RECORD_REG_CNT"/>
		<result property="recordCnt" 		column="RECORD_CNT"/>
		<result property="recordTotPageCnt" column="RECORD_TOT_PAGE_CNT"/>
		<result property="recordPageCnt" 	column="RECORD_PAGE_CNT"/>
	</resultMap>
	<select id="findErrorRecordRegCnt" parameterClass="rms.take.oldElectReg.domain.FolderOldElectReg" resultMap="findErrorRecordReg-result">
	    select 	  record_reg_cnt
		        , lpad((select count(1) 
		        		  from TB_TKRECORD b 
		        		 where a.folder_take_seq = b.folder_take_seq), 6,'0') record_cnt
		        , record_tot_page_cnt
		        , lpad((select count(1) 
		        		  from TB_TKRECORD b, TB_TKATTACHFILE c 
		        		 where b.record_center_id = c.record_center_id 
		        		 and b.record_take_seq = c.record_take_seq 
		        		 and b.folder_take_seq = a.folder_take_seq ), 6, '0')record_page_cnt
		        , line_no
		  from TB_TKFOLDER a
		 where record_center_id = #recordCenterId:VARCHAR#
		   and creat_yyyy 		= #creatYyyy:VARCHAR# 
		   and creat_sys_cd 	= #creatSysCd:VARCHAR#
		   and reg_dtime 		= #regDtime:VARCHAR#
		   and (record_reg_cnt != lpad((select count(1) 
		   								  from TB_TKRECORD b 
		   								 where a.folder_take_seq = b.folder_take_seq), 6,'0') 
		        or record_tot_page_cnt != lpad((select count(1) 
		        								  from TB_TKRECORD b, TB_TKATTACHFILE c 
		        								 where b.record_center_id = c.record_center_id 
		        								   and b.record_take_seq = c.record_take_seq 
		        								   and b.folder_take_seq = a.folder_take_seq ), 6, '0'))
	</select>
	<resultMap id="findErrorErecordFlag-result" class="rms.take.oldElectReg.domain.RecordOldElectReg">
		<result property="lineNo" column="LINE_NO"/>
		<result property="erecordFlag" column="ERECORD_FLAG"/>
		<result property="creatSysCd" column="CREAT_SYS_CD"/>
		<result property="recordCenterId" column="RECORD_CENTER_ID"/>
		<result property="creatYyyy" column="CREAT_YYYY"/>
		<result property="regDtime" column="REG_DTIME"/>
	</resultMap>
	<select id="findErrorErecordFlag" parameterClass="rms.take.oldElectReg.domain.RecordOldElectReg" resultMap="findErrorErecordFlag-result">
	select    line_no
			, erecord_flag
			, creat_sys_cd
			, record_center_id
			, creat_yyyy
			, reg_dtime
	  from (
			select    a.erecord_flag
					, (select count(1) 
						 from tb_tkattachfile b 
						where b.record_center_id 	= a.record_center_id 
						  and b.record_take_seq 	= a.record_take_seq 
						  and doc_type_cd='01') cnt
					, a.line_no
					, a.creat_sys_cd
					, a.record_center_id
					, a.creat_yyyy
					, reg_dtime
					, org_cd
			  from TB_TKRECORD a
			 where a.record_center_id 	= #recordCenterId:VARCHAR#
			   and a.creat_sys_cd 		= #creatSysCd:VARCHAR#
			   and reg_dtime 			= #regDtime:VARCHAR#
			)
	 where (erecord_flag ='1' and cnt = 0) or (erecord_flag='2' and cnt <![CDATA[>]]> 0)
	</select>
	<update id="updateRecordAttachCnt" parameterClass="rms.take.oldElectReg.domain.OldElectReg">
		update TB_TKRECORD A
		   set A.attach_cnt = (select count(1) 
		  						from TB_TKATTACHFILE B 
		  					   where B.record_center_id = #recordCenterId:VARCHAR# 
		  					     and B.reg_dtime = #recvDtime:VARCHAR#
		  					     and A.record_center_id = B.record_center_id
		  					     and A.record_take_seq = B.record_take_seq)
		 where A.record_center_id = #recordCenterId:VARCHAR#
		   and A.creat_sys_cd = #creatSysCd:VARCHAR#
		   and A.reg_dtime = #recvDtime:VARCHAR# 
	</update>
	
	<!-- 파일 등록 순번 조회 -->
	<select id="findRegSeq" parameterClass="rms.take.oldElectReg.domain.AttachFileOldElect" resultClass="string">
		select nvl(max(reg_seq) + 1, 1) regSeq 
		  from tb_tkattachfile 
		 where record_center_id = #recordCenterId:VARCHAR#
		   and record_take_seq = #recordTakeSeq:VARCHAR#
		   and attach_file_type = #attachFileType:VARCHAR#  
	</select>
		<update id="updateRecordSize" parameterClass="rms.take.oldElectReg.domain.OldElectReg">
		update tb_tkrecord a 
		   set a.record_size = (select nvl(file_size, 0) 
		                          from (select record_take_seq, nvl(sum(file_size), 0) file_size
		                                  from tb_tkattachfile
	                                     where record_center_id = #recordCenterId:VARCHAR#
		                                   and reg_dtime = #recvDtime:VARcHAR#
		                                 group by record_take_seq
		                               )
		                         where record_take_seq = a.record_take_seq 
		                       )                         
		 where a.record_center_id = #recordCenterId:VARcHAR#
		   and a.creat_sys_cd = #creatSysCd:VARcHAR#
		   and a.reg_dtime = #recvDtime:VARCHAR#
		   and attach_cnt <![CDATA[>]]> 0
	</update>
	<update id="updateFolderSize" parameterClass="rms.take.oldElectReg.domain.OldElectReg">
		 update tb_tkfolder a
		    set a.folder_size = (select nvl(folder_size, 0) 
		                           from ( select folder_take_seq, nvl(sum(record_size),0) folder_size
		                                    from tb_tkrecord 
		                                   where record_center_id = #recordCenterId:VARCHAR#
		                                     and creat_sys_cd = #creatSysCd:VARCHAR#
		                                     and reg_dtime = #recvDtime:VARcHAR#
		                                   group by folder_take_seq
		                                 )   
		                           where folder_take_seq = a.folder_take_seq
		                         )
		  where record_center_id = #recordCenterId:VARcHAR#
		    and creat_sys_cd = #creatSysCd:VARCHAR#
		    and reg_dtime = #recvDtime:VARCHAR#
		    and exists (
		    	select 1 from tb_tkrecord 
		    	where 	record_center_id = #recordCenterId:VARCHAR#
		    	and 	creat_sys_cd = #creatSysCd:VARCHAR#
		    	and 	reg_dtime = #recvDtime:VARCHAR#
		    )
	</update>
	
	<select id="findRegProgStateCd" parameterClass="rms.take.oldElectReg.domain.OldElectRegFile" resultClass="string">
		select max(reg_prog_state_cd) 
		from tb_tkrecvhist
		where record_center_id = #recordCenterId:VARCHAR#
		and recv_dtime = #regDtime:VARCHAR#
	</select>
	
</sqlMap>