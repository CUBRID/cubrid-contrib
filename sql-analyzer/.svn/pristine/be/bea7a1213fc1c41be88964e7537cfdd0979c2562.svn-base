-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\common\detail\DetailCrRecord.xml
-- Query    : select
-- Query Id : findCrRecordKeywdList

		SELECT REPLACE(REPLACE(KEYWD,'/',','),',,',',') keywd		FROM (			SELECT SUBSTR (MAX (SYS_CONNECT_BY_PATH (REPLACE(KEYWD,',','/'), ',')), 2) keywd			FROM (				SELECT spcl_list1||DECODE(NVL(spcl_list2,''),'','','/'||spcl_list2)							||DECODE(NVL(spcl_list3,''),'','','/'||spcl_list3) keywd					, record_recv_seq					, rownum rnum				FROM TB_TKSPCLLIST A 				WHERE A.record_center_id = '1'				AND   A.record_recv_seq = '1'				ORDER BY A.seq			)			START WITH rnum = 1			CONNECT BY PRIOR rnum = rnum - 1 AND prior record_recv_seq = record_recv_seq		);


