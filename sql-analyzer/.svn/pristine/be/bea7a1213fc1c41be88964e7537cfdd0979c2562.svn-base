-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\common\detail\DetailCrRecord.xml
-- Query    : select
-- Query Id : findCrRecordFileList

		SELECT ROWNUM rnum, ROWNUM idx, TA.*		FROM (			SELECT A.record_center_id				, A.record_recv_seq				, A.reg_seq				, A.doc_type_cd				, Z.com_cd_nm doc_type_nm				, (A.creat_yyyy||A.creat_reg_no||A.attach_file_seq||DECODE(doc_type_cd,'01','N','S')) AS file_nm				, '' agent_nm				, A.attach_file_page_cnt				, '' attach_file_path				, CEIL(NVL(A.file_size/1024,0)) file_size				, '' ver			FROM  TB_CRATTACHFILE A			    , (SELECT com_cd, com_cd_nm FROM TB_ZZCOMCD WHERE com_type_cd = 'RD21') Z			WHERE A.record_center_id = '1'			AND   A.record_recv_seq = '1'			 AND 				A.doc_type_cd  = '1'			 			AND   A.doc_type_cd = Z.com_cd(+)			ORDER BY A.doc_type_cd, A.doc_type_sno		) TA	;


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\common\detail\DetailCrRecord.xml
-- Query    : select
-- Query Id : findCrRecordFileList01

		SELECT ROWNUM rnum, ROWNUM idx, TA.*		FROM (			SELECT A.record_center_id				, A.record_recv_seq				, A.reg_seq				, A.doc_type_cd				, Z.com_cd_nm doc_type_nm				, (A.creat_yyyy||A.creat_reg_no||A.attach_file_seq||DECODE(doc_type_cd,'01','N','S')) AS file_nm				, '' agent_nm				, A.attach_file_page_cnt				, '' attach_file_path				, CEIL(NVL(A.file_size/1024,0)) file_size				, '' ver			FROM  TB_CRATTACHFILE A			    , (SELECT com_cd, com_cd_nm FROM TB_ZZCOMCD WHERE com_type_cd = 'RD21') Z			WHERE A.record_center_id = '1'			AND   A.record_recv_seq = '1'			AND   A.doc_type_cd IN ('01','05')			AND   A.doc_type_cd = Z.com_cd(+)			ORDER BY A.doc_type_cd, A.doc_type_sno		) TA	;


