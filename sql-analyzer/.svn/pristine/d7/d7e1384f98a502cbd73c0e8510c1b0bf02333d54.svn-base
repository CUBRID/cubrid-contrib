-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\sys\recordmng\CurKeepOrgMng.xml
-- Query    : select
-- Query Id : findCurKeepOrgMngClssPagedList

		SELECT E.*		FROM (		    SELECT D.*		    FROM (		        SELECT ROWNUM as rnum		            ,C.*		        FROM (		           SELECT record_center_id, cur_keep_org_cd, cur_keep_org_nm, clss_id, clss_nm, count(1) as folder_cnt		            FROM (		                    SELECT A.record_center_id,		                           A.cur_keep_org_cd,		                           (SELECT org_nm FROM TB_STORG WHERE record_center_id = A.record_center_id AND org_cd = A.cur_keep_org_cd) as cur_keep_org_nm,		                           A.clss_id,		                           DECODE (		                                   A.clss_div_cd, '1',		                                   (SELECT bis_clss_nm FROM TB_ZZBISCLSS Z WHERE Z.bis_clss_id = A.clss_id), '2',		                                   (SELECT unit_nm FROM TB_ZZUNIT Y WHERE Y.record_center_id = A.record_center_id AND Y.unit_cd = A.clss_id) , '3', '±‚≈∏'		                           ) as clss_nm		                    FROM TB_RDFOLDER A		                    WHERE A.record_center_id = '1'		                    )		            GROUP BY record_center_id, cur_keep_org_cd, cur_keep_org_nm, clss_id, clss_nm		        ) C		        WHERE ROWNUM <= '1'		    ) D		    WHERE D.rnum >= '1'		) E		ORDER BY E.rnum	;


