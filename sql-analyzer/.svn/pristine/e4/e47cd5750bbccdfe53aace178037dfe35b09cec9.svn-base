					AND   A.folder_keep_state_cd IN ('01','04')
		            GROUP BY A.folder_type_div_cd, A.folder_detl_type_cd
		        ) C
		        <dynamic prepend="WHERE">
			        <isEqual prepend="AND" property="creatSysCd" compareValue="01">
