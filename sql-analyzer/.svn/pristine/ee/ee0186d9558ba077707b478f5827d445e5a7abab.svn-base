		SELECT A.info_src_type_cd			 , Z.com_cd_nm info_src_type_nm		     , A.info_src_sno		     , A.info_src_title		     , A.reg_dtime		FROM  TB_TKRECORDINFOSRC A			, (SELECT com_cd, com_cd_nm FROM TB_ZZCOMCD WHERE com_type_cd = 'RD47') Z		WHERE A.record_center_id = '1'		AND   A.record_take_seq = '1'		AND   A.info_src_type_cd = Z.com_cd(+);


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\common\detail\DetailTkRecord.xml
-- Query    : select
-- Query Id : findTkRecordKeywdList

		SELECT REPLACE(REPLACE(KEYWD,'/',','),',,',',') keywd		FROM (			SELECT SUBSTR (MAX (SYS_CONNECT_BY_PATH (REPLACE(KEYWD,',','/'), ',')), 2) keywd			FROM (				SELECT spcl_list1||DECODE(NVL(spcl_list2,''),'','','/'||spcl_list2)							||DECODE(NVL(spcl_list3,''),'','','/'||spcl_list3) keywd					, record_take_seq					, rownum rnum				FROM TB_TKSPCLLIST A 				WHERE A.record_center_id = '1'				AND   A.record_take_seq = '1'				ORDER BY A.seq			)			START WITH rnum = 1			CONNECT BY PRIOR rnum = rnum - 1 AND prior record_take_seq = record_take_seq		);


-- Filename : C:\eclipse\workspace\SqlMapXMLParser\rmsCubrid\rms\default\rms\common\detail\DetailTkRecord.xml
-- Query    : select
-- Query Id : findTkRecordQultyTestFileList

		SELECT ROW_NUMBER() OVER(PARTITION BY A.DOC_TYPE_CD ORDER BY A.DOC_TYPE_CD, A.REG_SEQ) AS ROW_IDX		      ,COUNT(*) OVER(PARTITION BY A.DOC_TYPE_CD) AS ROW_CNT		      ,A.DOC_TYPE_CD		      ,B.COM_CD_NM AS DOC_TYPE_NM		      ,A.ATTACH_FILE_TYPE		      ,A.REG_SEQ		      ,A.VIRUS_FLAG		      ,A.VIRUS_CLEAN_FLAG		      ,A.VIRUS_CLEAN_APPLY_FLAG		      ,DECODE(A.VIRUS_CLEAN_APPLY_FLAG, '1', '치료적용', '2', '치료미적용', DECODE(A.VIRUS_CLEAN_FLAG, '1', '치료', '2', '치료불가', DECODE(A.VIRUS_FLAG, '1', '감염', 'X'))) AS VIRUS_CLEAN_FLAG_NM		      ,A.FILE_NM		      ,A.FILE_SIZE		      ,A.ATTACH_FILE_PATH		      ,('TF' || '|' || A.ATTACH_FILE_TYPE || '|' || A.REG_SEQ) AS VIRUS_CLEAN_FILE_INFO		      ,A.VIRUS_CLEAN_FILE_SIZE		      ,A.VIRUS_CLEAN_FILE_PATH		FROM   (SELECT AA.DOC_TYPE_CD		              ,AA.ATTACH_FILE_TYPE		              ,AA.REG_SEQ		              ,AA.FILE_NM		              ,AA.ATTACH_FILE_PATH		              ,(NVL(AA.FILE_SIZE, 0) / 1024) AS FILE_SIZE		              ,(NVL(AA.VIRUS_CLEAN_FILE_SIZE, 0) / 1024) AS VIRUS_CLEAN_FILE_SIZE		              ,AA.VIRUS_CLEAN_FILE_PATH		              ,AA.VIRUS_FLAG		              ,AA.VIRUS_CLEAN_FLAG		              ,AA.VIRUS_CLEAN_APPLY_FLAG		        FROM   TB_TKATTACHFILE AA		              ,TB_TKRECORD     AB		        WHERE  AA.RECORD_CENTER_ID = AB.RECORD_CENTER_ID		        AND    AA.RECORD_TAKE_SEQ = AB.RECORD_TAKE_SEQ		        AND    AA.RECORD_CENTER_ID = '1'		        AND    AA.RECORD_TAKE_SEQ = '1'		        AND    AB.ERECORD_FLAG = '1') A		      ,(SELECT COM_CD, COM_CD_NM FROM TB_ZZCOMCD WHERE COM_TYPE_CD = 'RD21') B		WHERE  A.DOC_TYPE_CD = B.COM_CD(+);


