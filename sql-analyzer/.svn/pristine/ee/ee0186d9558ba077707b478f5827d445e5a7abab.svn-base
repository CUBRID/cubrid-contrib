		SELECT ROWNUM rnum, TA.*		FROM (			SELECT A.agent_id			     , A.agent_type_cd			     , (CASE WHEN A.agent_type_cd='08' THEN '보고' WHEN A.agent_type_cd='04' THEN '참조' ELSE '' END) agent_type_nm			     , TRIM(A.agent_nm) agent_nm			     , TRIM(A.agent_org_nm) agent_org_nm				 , A.agent_pos_rank_nm				 , A.req_state_val				 , (CASE WHEN A.duty_act_dtime='11111111111111' THEN '' ELSE RPAD(A.duty_act_dtime,14,'0') END) duty_act_dtime				 , A.opin				 , A.handl_rslt				 , A.agent_duty_type_cd				 , '' ver				 , A.record_center_id				 , A.record_take_seq				 , '' reg_seq			FROM  TB_TKRECORDCREAT A			WHERE A.record_center_id = '1'			AND   A.record_take_seq = '1'			AND   A.agent_type_cd IN ('08','04')			ORDER BY A.duty_act_dtime, A.agent_sno		) TA	;


