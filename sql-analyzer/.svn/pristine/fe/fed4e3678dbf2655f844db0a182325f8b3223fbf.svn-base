R_ID = O.RECORD_CENTER_ID		AND R.ORG_CD = O.ORG_CD		AND R.OPER_READ_RANGE_CD = COR.COM_CD(+)		AND		COR.COM_TYPE_CD(+) = 'RD53'		AND F.CLSS_DIV_CD = C0.COM_CD		AND		C0.COM_TYPE_CD = 'RD19'		AND R.CREAT_SYS_CD = C1.COM_CD		AND		C1.COM_TYPE_CD = 'RD17'		AND R.RECORD_REG_DIV_CD = C2.COM_CD(+)		AND		C2.COM_TYPE_CD(+) = 'RD01'		AND R.RECORD_REG_DIV_CD = C6.COM_CD(+)		AND		C6.COM_TYPE_CD(+) = 'RD02'		AND R.RECORD_DETL_TYPE_CD = C3.COM_CD(+)		AND		C3.COM_TYPE_CD(+) = 'RD03'		AND R.MEDIA_FORMAT = C4.COM_CD(+)		AND		C4.COM_TYPE_CD(+) = 'RD15'		AND R.SPCL_RECORD_TYPE = C5.COM_CD(+)		AND		C5.COM_TYPE_CD(+) = 'RD18'		AND R.SECU_CLSS_CD = C7.COM_CD(+)		AND		C7.COM_TYPE_CD(+) = 'RD11'		AND R.OPEN_DIV_CD = C8.COM_CD		AND		C8.COM_TYPE_CD = 'RD05'		AND R.OPEN_GRADE = C9.COM_CD(+)		AND		C9.COM_TYPE_CD(+) = 'RD06'		AND R.MEDIUM = C10.COM_CD(+)		AND		C10.COM_TYPE_CD(+) = 'RD67';

